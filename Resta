package calculadora;

import java.util.Scanner;

/**
 *
 * @author LINCOLN
 */
public class Restas {

    public static void main(String[] args) {
        // TODO Auto-generated method stub

        Scanner obtenerNumero = new Scanner(System.in);
        int numero, i, j;

        System.out.print("indica el numero que desea que \n"
                + " se realize la operaci√≥n de resta: ");
        numero = obtenerNumero.nextInt();

        for (i = 1; i <= numero; i++) {
            for (j = 1; j <= 12; j++) {
                System.out.println((i + " - " + j + " = " + (i - j)));
            }
            System.out.println();

        }
    }
}
